  },
  "homepage": "https://github.com/marigold-dev/tzstamp/tree/main/cli#readme",
  "dependencies": {
    "@tzstamp/helpers": "^0.3.3",
    "@tzstamp/proof": "^0.3.3",
    "chalk": "^4.1.1",
    "delay": "^5.0.0",
    "minimist": "^1.2.5",
    "node-fetch": "^2.6.1"
  },
  "pkg": {
